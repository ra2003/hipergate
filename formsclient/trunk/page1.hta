<html>
	<head>
		<title>Data Entry Forms Example</title>
		<HTA:APPLICATION ID="oHTA"
			APPLICATIONNAME="DataEntryForms"
			BORDER="thick"
			BORDERSTYLE="normal"
			CAPTION="yes"
			CONTEXTMENU="no"
			ICON="_img/knowgate.ico"
			INNERBORDER="yes"
			MAXIMIZEBUTTON="no"
			MINIMIZEBUTTON="yes"
			NAVIGABLE="yes"
			SCROLL="no"
			SCROLLFLAT="yes"
			SELECTION="no"
			SHOWINTASKBAR="yes"
			SINGLEINSTANCE="yes"
			SYSMENU="yes"
			VERSION="1"
			WINDOWSTATE="maximize"
		/>
		<meta http-equiv="content-type" content="application/hta; charset=iso-8859-1">
		<link rel="stylesheet" type="text/css" href="_img/styles.css" />
		<script type="text/javascript" language="JavaScript" src="_js/globvars.js"></script>
		<script type="text/javascript" language="JavaScript" src="_js/resources_es.js"></script>
		<script type="text/javascript" language="JavaScript" src="_js/formutils.js"></script>
		<script type="text/javascript" language="JavaScript" src="_js/vbautils.js"></script>
		<script type="text/javascript" language="JavaScript" src="_js/xmlhttprequest.js"></script>
		<script type="text/javascript" language="JavaScript" src="_js/autosuggest_city.js"></script>
		<script type="text/javascript" language="JavaScript">
		<!--

			// Set here the name of the next page at forms chain
			var action_target = "page2.hta";

		  function initialize() {		  	
		    var frm = document.forms[0];

