<mymw:document id="login" xmlns:mymw="http://morfeo-project.org/mymobileweb">
  <mymw:head>
  
    <mymw:title class="noinclude">hipergate : Login</mymw:title>    
   
  </mymw:head>
  
  <mymw:body newcontext="true">
    <mymw:div id="p1" class="center grid2">
      <mymw:label class="right">e-mail:</mymw:label>
      <mymw:entryfield id="USER" bind="${ _MYMW_LOGIN }" class="left login" />
      <mymw:label class="right">Clave:</mymw:label>
      <mymw:entryfield id="PASS" bind="${ _MYMW_PASS }" class="left password" />
    </mymw:div>
    
    <mymw:div id="p2" class="center">
      <mymw:submit id="_MYMW_LG_BT" value="Entrar" principal="true"/>
    </mymw:div>
    
  </mymw:body>
  
</mymw:document>