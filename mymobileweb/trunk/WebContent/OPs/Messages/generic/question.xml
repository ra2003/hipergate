  <mymw:head>
    <mymw:title>Error</mymw:title>
  </mymw:head>
  
  <mymw:body>
  	<mymw:div id="p1" class="center vertical">
       		<mymw:image id="ierror" resourceid="_MYMW_ERROR" alt="Error" />
